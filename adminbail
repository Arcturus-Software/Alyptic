-- Scripts:

local function GVCA_fake_script() -- AdminBail_2.LocalScript 
	local script = Instance.new('LocalScript', AdminBail_2)

	local whitelistTable = {}
	local whitelisted = false
	local plr = game:GetService("Players").LocalPlayer
	local plrId = plr.UserId
	
	
	
	
	
	
	
	
	
	local Whitelist = syn.request({
	    Url = "https://working-bot-arcturus.veloxdev.repl.co/static/adminlist.txt",
	    Method = "GET"
	})
	
	
	local main = script.Parent
	
	
	
	splitWhitelist = string.split(Whitelist.Body, ',')
	for i,v in pairs(splitWhitelist) do
	    table.insert(whitelistTable,v)
	end
	
	local det = true
	
	while task.wait(1) and det==true do
	
	local function search()
	    for i,v in pairs(whitelistTable) do
	        local id = tonumber(v)
	        
	        local function match()
	            if plrId == id then
	                return true
	            end
	        end
	        
	        if match() then
	            return true
	        end
	    end
	end
	if search() then
		det=false
	    whitelisted = true
		main.Visible=true
		script.bail.Disabled=false
		task.wait(1)
		main.Time.Text="9"
		task.wait(1)
		main.Time.Text="8"
		task.wait(1)
		main.Time.Text="7"
		task.wait(1)
		main.Time.Text="6"
		task.wait(1)
		main.Time.Text="5"
		task.wait(1)
		main.Time.Text="4"
		task.wait(1)
		main.Time.Text="3"
		task.wait(1)
		main.Time.Text="2"
		task.wait(1)
		main.Time.Text="1"
		task.wait(1)
		main.Time.Text="..."
	
		local ts = game:GetService("TeleportService")
	
	local p = game:GetService("Players").LocalPlayer
	
	 
	
	ts:Teleport(game.PlaceId, p)
	else
	    whitelisted = false
	   	main:Destroy()
	end
	print(whitelisted)
	
	end
end
coroutine.wrap(GVCA_fake_script)()
-- nil.bail is disabled.
local function YSMCA_fake_script() -- AdminBail_2.53 
	local script = Instance.new('LocalScript', AdminBail_2)

	 one = Instance.new('UIStroke');
	
	one.Parent = script.Parent
	one.Enabled=true
	one.Transparency = 0
	one.LineJoinMode = "Round"
	one.Thickness=2
	one.ApplyStrokeMode="Contextual"
	one.Color = Color3.fromRGB(255, 255, 255)
end
coroutine.wrap(YSMCA_fake_script)()
